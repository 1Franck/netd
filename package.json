{
  "name": "NETD",
  "version": "1.0.0",
  "description": "Node+Express+TypeScript+Docker",
  "license": "MIT",
  "scripts": {
    "tests": "mocha -r ts-node/register 'tests/**/*.spec.ts'",
    "start:dev": "node --require ts-node/register ./src/main",
    "start:dev-watch": "nodemon --legacy-watch",
    "build:dev": "npm install --no-bin-links",
    "build:prod": "npm install --no-bin-links && rm -rf dist && tsc --alwaysStrict",
    "start:prod": "forever dist/src/main.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "debug": "^4.1.0",
    "express": "^4.16.3",
    "helmet": "^3.15.0",
    "reflect-metadata": "^0.1.12",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "ioredis": "^4.3.0",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/chai-http": "^3.0.5",
    "@types/compression": "0.0.36",
    "@types/debug": "0.0.30",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.38",
    "@types/ioredis": "^3.2.8",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.3.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "nock": "^9.6.1",
    "nodemon": "^1.17.5",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0",
    "typescript": "^3.2"
  },
  "author": "",
  "contributors": []
}
